@model DetalheJogoModel

@{
    ViewBag.Title = "Index";
}

@Html.Partial("~/Views/Shared/Locacao/_InformacoesJogo.cshtml", Model)

<form action="@Url.Action("Locar", "Locacao")" method="get">
    <label for="nome">Pesquisa por nome: </label>
    <input type="text" id="nome" name="nome" value="" />

    <input type="submit" id="btnPesquisar" value="Pesquisar" />
</form>

@section Scripts {
    <script>
        $("document").ready(function() {
            $("#nome").autocomplete({
              source: '@Url.Action("ClientesAutoComplete", "Cliente")',
              //select: function() { $("#btnPesquisar").click(); }
            });
        });
    </script>
}